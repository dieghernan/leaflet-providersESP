{
  "name": "leaflet-providersESP",
  "version": "1.0.0",
  "description": "An extension to Leaflet that contains configurations for various free WMS/WMTS tile providers of Spain.",
  "main": "dist:leaflet-providersESP.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/dieghernan/leaflet-providersESP.git"
  },
  "scripts": {
    "test": "npm run lint && npm run testsuite",
    "testsuite": "mocha-chrome tests/index.html",
    "lint": "eslint --config .eslintrc dist/leaflet-providersESP.js",
    "min": "uglifyjs dist/leaflet-providersESP.js -mc -o dist/leaflet-providersESP.min.js",
    "release-patch": "mversion patch -m",
    "release-minor": "mversion minor -m"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dieghernan/leaflet-providersESP/issues"
  },
  "files": [
    "dist/leaflet-providersESP.js",
    "README.md",
    "CHANGELOG.md"
    "LICENSE"
  ],
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^5.4.0",
    "eslint-plugin-html": "^5.0.3",
    "mocha": "^5.2.0",
    "mocha-chrome": "^1.1.0",
    "mversion": "^2.0.0",
    "uglify-js": "^2.8.29"
  },
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/dieghernan/leaflet-providersESP.git",
    "basePath": "/",
    "files": [
      "dist/leaflet-providersESP.js"
    ]
  }
}
